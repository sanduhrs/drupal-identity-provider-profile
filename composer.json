        "drupal/simplesamlphp_auth": "^3.2",
        "drupal/tagged_routes": "^1.0",
        "drupal/webauthn": "@dev",
        "drupal/webhooks": "@dev",
        "mglaman/drupal-check": "^1.1",
        "mnsami/composer-custom-directory-installer": "^2.0",
        "npm-asset/jquery-circle-progress": "^1.2",
        "npm-asset/web-auth--webauthn-helper": "^0.0.13",
