        "drupal/openapi_ui": "@dev",
        "drupal/openapi_ui_swagger": "@dev",
        "drupal/password_policy": "^3.0",
        "drupal/pathauto": "^1.8",
        "drupal/pwa": "^1.5",
        "drupal/rate_limits": "^2.0",
        "drupal/real_aes": "^2.3",
        "drupal/restui": "^1.20",
        "drupal/schemata": "@dev",
        "drupal/schemata_json_schema": "@dev",
