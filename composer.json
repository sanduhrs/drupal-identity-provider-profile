    "require": {
        "ext-json": "*",
        "drupal/admin_toolbar": "^3.0",
        "drupal/authorization": "^1@dev",
        "drupal/core-composer-scaffold": "^9.2",
        "drupal/core-recommended": "^9.2",
        "drupal/csp": "^1.15",
        "drupal/encrypt": "^3.0",
        "drupal/flood_control": "^2.1",
        "drupal/gin": "@dev",
        "drupal/gin_lb": "@dev",
        "drupal/gin_login": "@dev",
        "drupal/gin_toolbar": "@dev",
        "drupal/jsonapi_extras": "^3.19",
        "drupal/jsonapi_hypermedia": "^1.6",
        "drupal/jsonapi_include": "^1.4",
        "drupal/jsonrpc": "^2.0",
        "drupal/ldap": "^4@dev",
        "drupal/ldap_servers": "^4@dev",
        "drupal/login_security": "^2.0",
        "drupal/mail_login": "^2.4",
        "drupal/oauth2_server": "@dev",
